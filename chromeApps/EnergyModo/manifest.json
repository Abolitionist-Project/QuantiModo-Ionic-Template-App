{
  "name": "Energy Modo",
  "version": "0.0.1",
  "manifest_version": 2,
  "minimum_chrome_version": "33",
  "icons": {
      "16"	: "www/img/icon_energymodo_16.png",
      "48"	: "www/img/icon_energymodo_48.png",
      "128"	: "www/img/icon_energymodo_128.png"
  },
  "app": {
    "background": {
      "scripts": ["scripts/background.js"]
    }
  },
  "permissions": [
    "identity",
    "geolocation",
    "storage",
    "notifications",
    "alarms",

    "http://app.quantimo.do/*"
  ]
}
